# OmniASR Backend Configuration

# API Keys (comma-separated, leave empty to disable authentication)
# Example: API_KEYS=key1,key2,key3
API_KEYS=

# Allowed CORS Origins (comma-separated)
# Example: ALLOWED_ORIGINS=https://yourdomain.com,http://localhost:5173
ALLOWED_ORIGINS=*

# Model Configuration
MODEL_CARD=omniASR_LLM_7B_v2
MODEL_DIR=/model

# File Upload Limits
MAX_FILE_SIZE_MB=100

# Rate Limiting (requests per minute)
RATE_LIMIT_HEALTH=30
RATE_LIMIT_LANGUAGES=10
RATE_LIMIT_TRANSCRIBE=20
RATE_LIMIT_TRANSCRIBE_LARGE=10

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
